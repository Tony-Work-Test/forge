{
  "name": "drc-forge",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/drc-forge/src",
  "projectType": "application",
  "targets": {
    "register": {
      "executor": "@toolsplus/nx-forge:register",
      "options": {
        "outputPath": "dist/apps/drc-forge"
      }
    },
    "build": {
      "executor": "@toolsplus/nx-forge:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/drc-forge",
        "webpackConfig": "apps/drc-forge/webpack.config.js"
      }
    },
    "serve": {
      "executor": "@toolsplus/nx-forge:tunnel",
      "options": {
        "outputPath": "dist/apps/drc-forge"
      }
    },
    "deploy": {
      "executor": "@toolsplus/nx-forge:deploy",
      "options": {
        "outputPath": "dist/apps/drc-forge"
      }
    },
    "install": {
      "executor": "@toolsplus/nx-forge:install",
      "options": {
        "outputPath": "dist/apps/drc-forge"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/drc-forge/jest.config.ts"
      }
    }
  },
  "tags": []
}
